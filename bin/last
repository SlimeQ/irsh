#!/usr/bin/fish

. lib/opts.fish

opts r $argv

set raw

set i 1
while test $i -le (count $flags)
    switch $flags[$i]
        case '-r'
            set raw 1
    end
    set i (math $i+1)
end

set nick (echo "$pos" | tr -d '/')
if test -n "$raw"
    cat "var/last/$nick" | sed 's/\S\+ \S\+ \(.*\)/\1/'
else
    cat "var/last/$nick"
end
