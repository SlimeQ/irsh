#!/usr/bin/fish

. lib/sudo.fish

if sudoer
    if test (count $argv) -eq 2
        set src (lib/path $argv[1])
        set dest (lib/path $argv[2])
        if not test -f $dest
            mv $src $dest
        else
            echo destination exists
        end
    else
        bin/man (basename (status -f))
        exit 1
    end
end
